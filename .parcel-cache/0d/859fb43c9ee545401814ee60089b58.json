</Link></li>\n\t\t\t</ul>\n\t\t</nav>\n\n\t\t<Switch>\n\t\t\t<Route exact path= '/'>\n\t\t\t\t<Home />\n\t\t\t</Route>\n\t\t\t\n\t\t\t<Route exact path= '/blog'>\n\t\t\t\t<BlogListPage />\n\t\t\t</Route>\n\t\t\t<Route exact path= '/blog/:slug'>\n\t\t\t\t<BlogDetailPage />\n\t\t\t</Route>\n\n\t\t\t<Route exact path= '/contact'>\n\t\t\t\t<h1>Contact Form</h1>\n\t\t\t\t<ContactPage />\n\t\t\t</Route>\n\t\t</Switch>\n\n\t</div> \t\n}\n\n\n\n\n\n/*const App = () => {\n\n\tconst [count, setCount] = useState(5001)\n\tconst handleClick = (event) =>{\n\t\tsetCount(count + 1)\n\t}\n\n\tconst handleCountUpadate = (newCount) =>{\n\t\tif (count < newCount){\n\t\t\tsetCount(newCount)\n\t\t}\n\t}\n\treturn <div>\n\t\t<h1> Hello World </h1>\n\t\t<button onClick = {handleClick}>My incrementor</button>\n\t\t<LikeBtn className = 'text-red' initCount = {count} \n\t\t\tonCountUpdate = {handleCountUpadate}>Perform Like</LikeBtn>\n\t</div> \t\n}*/\n\nexport default App"},"lineCount":null}},"error":null,"hash":"c99506b7dfce80d1bc8c92c09a671011","cacheData":{"env":{}}}